page {
  height: 100%;
}

.anu-row {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  flex: 1;
}

.anu-col {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.anu-col-flex {
  flex-direction: column;
}

.anu-center {
  justify-content: center;
}

.anu-middle {
  align-items: center;
}

.anu-flex-center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.calendar-content {
  width: 100%;
}
.calendar-content .m-calendar {
  margin-top: 21px;
  background-color: #fff;
  overflow: hidden;
}
.calendar-content .e-head {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 10;
  background-color: #fff;
  border-bottom: 1rpx solid #e5e5e5;
  display: flex;
  height: 21px;
}
.calendar-content .e-head .w {
  flex: 1;
  font-size: 24rpx;
  text-align: center;
  height: 21px;
  line-height: 21px;
  align-items: center;
  justify-content: center;
}
.calendar-content .e-head .s {
  color: #fe9808;
}
.calendar-content .e-month {
  background: #fff;
}
.calendar-content .e-month .b-header {
  text-align: center;
  font-size: 18px;
  height: 30px;
  line-height: 30px;
  padding-top: 8px;
}
.calendar-content .e-month .b-header:first-child {
  border-top: 0;
}
.calendar-content .e-month .b-row {
  margin: 0 2px;
  display: flex;
  flex-wrap: wrap;
}
.calendar-content .e-month .b-row .item {
  width: 14.28%;
  margin-bottom: 5px;
  height: 60px;
  border-radius: 4px;
}
.calendar-content .multi .e-month .b-row .item.select,
.calendar-content .multi .e-month .b-row .item-a:active {
  background-color: #f5f5f5;
}
.calendar-content .multi .e-month .b-row .item.select .dayclass,
.calendar-content .multi .e-month .b-row .item.select .holiday,
.calendar-content .multi .e-month .b-row .item.select .ban,
.calendar-content .multi .e-month .b-row .item.select .xiu,
.calendar-content .multi .e-month .b-row .item.select .text,
.calendar-content .multi .e-month .b-row .item-a:active {
  color: #00bcd4;
}
.calendar-content .e-month .b-row .text,
.calendar-content .e-month .b-row .highlight {
  font-size: 11px;
  transform: scale(0.92);
  text-align: center;
  height: 20px;
  color: #666;
  width: 100%;
  color: #888;
}
.calendar-content .e-month .b-row .text .rmb {
  color: #888;
}
.calendar-content .e-month .b-row .highlight,
.calendar-content .e-month .b-row .highlight .rmb {
  color: #fe9808;
}
.calendar-content .e-month .b-row .holiday,
.calendar-content .e-month .b-row .ban,
.calendar-content .e-month .b-row .xiu {
  display: block;
  width: 100%;
  font-size: 11px;
  transform: scale(0.92);
  color: #fe9808;
  text-align: center;
}
.calendar-content .e-month .b-row .base {
  height: 20px;
  line-height: 20px;
}
.calendar-content .e-month .b-row .ban,
.calendar-content .e-month .b-row .holiday {
  color: #bbb;
}
.calendar-content .e-month .b-row .disabled,
.calendar-content .e-month .b-row .disabled .holiday,
.calendar-content .e-month .b-row .disabled .ban,
.calendar-content .e-month .b-row .disabled .xiu {
  color: #c7ced4;
}
.calendar-content .e-month .b-row .weekend {
  color: #fe9808;
}
.calendar-content .e-month .b-row .select,
.calendar-content .e-month .b-row .item-a:active {
  color: #fff;
  background: #11b4ca;
}
.calendar-content .e-month .b-row .select-second {
  color: #fff;
  background: #ccc;
}
.calendar-content .e-month .b-row .select .holiday,
.calendar-content .e-month .b-row .select .ban,
.calendar-content .e-month .b-row .select .xiu,
.calendar-content .e-month .b-row .select .text,
.calendar-content .e-month .b-row .select .text .rmb {
  color: #fff;
}
.calendar-content .multi .e-month .b-row .disabled:active,
.calendar-content .e-month .b-row .disabled:active {
  color: #c7ced4;
  background: #fff;
}
.calendar-content .e-btn {
  position: fixed;
  z-index: 9999;
  bottom: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 17px;
  background-color: #ff9705;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.calendar-content .e-btn .textColor {
  color: #fff;
}
.calendar-content .e-btn:active {
  background-color: #cc7904;
}
.calendar-content .e-btn-padding {
  height: 50px;
}