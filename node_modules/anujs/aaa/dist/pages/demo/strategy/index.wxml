
    <view class="strategy-page">
        <block wx:for="{{state.data}}" wx:for-item="item" wx:for-index="i971" wx:key="*this">
            <view bindtap="dispatchEvent" class="strategy-item col" data-tap-uid="{{'e45_27_' + i971}}" data-beacon-uid="default">
                <image class="big-image" src="{{item.bigImage}}" />
                <view class="strategy-item-content">
                    <view class="col"><text class="desc fontColor">{{item.desc}}</text><text class="date fontColor">{{item.date}}</text></view>
                    <view class="user-wrapper">
                        <view class="user-image">
                            <image class="image" mode="scaleToFill" src="{{item.userImage}}" />
                        </view><text class="user-name fontColor">{{item.userName}}</text>
                    </view>
                </view>
            </view>
        </block>
    </view>
